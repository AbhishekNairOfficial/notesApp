  "scripts": {
    "start": "better-npm-run start",
    "start:prod": "better-npm-run start:prod",
    "build": "better-npm-run build",
    "build:prod": "better-npm-run build:prod",
    "lint": "eslint --ext .js,.jsx .",
    "test": "karma start"
  },
  "betterScripts": {
    "build": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "node server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:prod": {
      "command": "node server.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  }
}
