    "babel-runtime": "5.8.20",
    "better-npm-run": "0.0.2",
    "classnames": "^2.1.2",
    "deasync": "^0.1.2",
    "eslint": "^1.3.1",
    "eslint-config-airbnb": "0.0.8",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^3.2.3",
    "file-loader": "^0.8.4",
